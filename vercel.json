{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "build/**",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src": "/api",
            "dest": "index.js"
        }
        ,{
            "src": "/api/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/",
            "dest": "build/index.html"
        },
        {
            "src": "/addProduct",
            "dest": "build/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "build/$1"
        }
    ]
}